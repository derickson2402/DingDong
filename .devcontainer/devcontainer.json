{
	"name": "PyDev",
	"dockerComposeFile": "docker-compose.yml",
	"service": "web",
	"workspaceFolder": "/workspaces/dingdong",
	"customizations": {
		"vscode": {
			"extensions": [
				"donjayamanne.python-extension-pack",
				"yzhang.markdown-all-in-one",
				"mtxr.sqltools",
				"ms-azuretools.vscode-docker",
				"fabiospampinato.vscode-diff",
				"nhoizey.gremlins",
				"mtxr.sqltools-driver-pg",
				"ms-python.python"
			]
		}
	},
	"postCreateCommand": "pip install -r src/cli/requirements.txt && pip install -r src/web/requirements.txt && pip install pigar"
}