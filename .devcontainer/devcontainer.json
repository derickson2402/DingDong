{
	"name": "PyDev",
	"image": "python:3.10",
	"customizations": {
		"vscode": {
			"extensions": [
				"donjayamanne.python-extension-pack",
				"yzhang.markdown-all-in-one",
				"mtxr.sqltools",
				"ms-azuretools.vscode-docker",
				"fabiospampinato.vscode-diff",
				"nhoizey.gremlins"
			]
		}
	},
	"postCreateCommand": "pip install -r src/cli/requirements.txt && pip install -r src/web/requirements.txt"
}